{
  "custom": {},
  "params": {
    "Title": "About/Settings"
  },
  "propConfig": {
    "params.Title": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 55,
      "width": 725
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Image"
        },
        "position": {
          "basis": "125px"
        },
        "props": {
          "fit": {
            "mode": "contain"
          },
          "source": "/system/images/DETRON-LOGO-FULL COLOR-REVERSED.PNG",
          "style": {
            "cursor": "pointer",
            "marginLeft": "5px",
            "marginRight": "5px"
          }
        },
        "type": "ia.display.image"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "50px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "fontSize": 16,
            "marginRight": "135px"
          },
          "textStyle": {
            "color": "#FFFFFF",
            "fontWeight": "bolder",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "backgroundColor": "#455C6C",
        "borderBottomStyle": "solid"
      }
    },
    "type": "ia.container.flex"
  }
}