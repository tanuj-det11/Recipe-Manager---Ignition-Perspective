          }
        },
        "type": "ia.chart.timeseries"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "borderStyle": "solid",
        "borderWidth": "2px",
        "margin": "3px",
        "overflow": "visible"
      }
    },
    "type": "ia.container.flex"
  }
