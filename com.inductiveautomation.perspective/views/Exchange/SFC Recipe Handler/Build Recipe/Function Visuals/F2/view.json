    "custom.startDate": {
      "binding": {
        "config": {
          "expression": "{view.params.Start}"
        },
        "overlayOptOut": true,
        "transforms": [
          {
            "code": "\tif value is None:\n\t\treturn 0\n\telse:\n\t\tstartdate\u003dsystem.date.parse(value, \"MMM dd, yyyy, h:mm:ss a\")\n\t\tif self.params.t1!\u003d\"\":\n\t\t\tdata\u003dsystem.tag.queryTagCalculations(paths\u003d[self.params.t1], calculations\u003d[\"LastValue\"], startDate\u003dsystem.date.addSeconds(startdate, -60), endDate\u003dstartdate)\n\t\t\tstartVal\u003ddata.getValueAt(0,1)\n\t\telse:\n\t\t\tstartVal\u003d0\n\t\tself.custom.startValue\u003dstartVal\n\t\taddseconds\u003d(self.params.p1-startVal)/self.params.p2\n\t\tif self.params.p3\u003d\u003d0:\n\t\t\taddseconds\u003daddseconds*3600\n\t\telif self.params.p3\u003d\u003d1:\n\t\t\taddseconds\u003daddseconds*60\n\t\telif self.params.p3\u003d\u003d2:\n\t\t\taddseconds\u003daddseconds*1\n\t\tself.custom.endTimeEst\u003dsystem.date.addSeconds(startdate, int(addseconds))\n\t\tself.custom.seconds\u003dsystem.date.secondsBetween(startdate, self.custom.endTimeEst)\n\t\treturn startdate",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
