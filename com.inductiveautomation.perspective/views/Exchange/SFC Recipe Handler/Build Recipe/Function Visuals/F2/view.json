          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Sparkline"
        },
        "position": {
          "basis": "100px",
          "grow": 1
        },
        "propConfig": {
          "props.desired.high": {
            "binding": {
              "config": {
                "expression": "max(tag({view.params.t1}),{view.custom.startValue})"
              },
              "type": "expr"
            }
          },
          "props.desired.low": {
            "binding": {
              "config": {
                "expression": "min(tag({view.params.t1}),{view.custom.startValue})"
              },
              "type": "expr"
            }
          },
          "props.points": {
            "binding": {
              "config": {
                "path": "view.custom.seconds"
              },
              "transforms": [
                {
                  "code": "\t#convert all params into per seconds\t\n\tif self.view.params.p3\u003d\u003d0:\n\t\ttconst\u003d3600.0\n\telif self.view.params.p3\u003d\u003d1:\n\t\ttconst\u003d60.0\n\telif self.view.params.p3\u003d\u003d2:\n\t\ttconst\u003d1.0\t\n\tstartrate\u003dself.view.params.p2\n\trate\u003dstartrate/tconst\n\t\n\tpoints\u003d[]\n\tp1\u003dself.view.params.p1\n\t\n\t#create a list of points replicating the ramp\n\tfor x in range(int(value)):\n\t\tpoint\u003dfloat(self.view.custom.startValue)+((x+1.0)*rate)\n\t\tpoints.append(point)\n\treturn points",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.range.high": {
            "binding": {
              "config": {
                "expression": "max({view.params.p1},{view.custom.startValue})+abs({view.params.p1}-{view.custom.startValue})*0.1"
              },
              "type": "expr"
            }
          },
          "props.range.low": {
            "binding": {
              "config": {
                "expression": "min({view.params.p1},{view.custom.startValue})-abs({view.params.p1}-{view.custom.startValue})*0.1"
              },
              "type": "expr"
            }
          }
        },
