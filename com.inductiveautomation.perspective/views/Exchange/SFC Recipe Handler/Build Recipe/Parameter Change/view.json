{
  "custom": {
    "function": 0,
    "unitSelected": ""
  },
  "params": {
    "function": 0,
    "output": 0,
    "param": "p1",
    "params": {
      "dropOptions": [
        {
          "label": "hr.",
          "value": 0
        },
        {
          "label": "min.",
          "value": 1
        },
        {
          "label": "sec.",
          "value": 2
        }
      ],
      "enabled": true,
      "unitSelect": false,
      "unit_enabled": false
    },
    "unit": ""
  },
  "propConfig": {
    "custom.function": {
      "binding": {
        "config": {
          "path": "view.params.function"
        },
        "type": "property"
      },
      "onChange": {
        "enabled": null,
        "script": "\t#changes the unit to set value for function 3 (soak)\n\tif currentValue.value\u003d\u003d3:\n\t\tif self.params.param\u003d\u003d\"p1\":\n\t\t\tself.getChild(\"root\").getChild(\"TextField\").props.text\u003d\"hr.\"\n\t\telif self.params.param\u003d\u003d\"p2\":\n\t\t\tself.getChild(\"root\").getChild(\"TextField\").props.text\u003d\"min.\"\n\t\telif self.params.param\u003d\u003d\"p3\":\n\t\t\tself.getChild(\"root\").getChild(\"TextField\").props.text\u003d\"sec.\"\n\t\n\t#changes the unit to set value for function 4 (guaranteed soak) parameter 3 (minutes)\n\telif currentValue.value\u003d\u003d4 and self.params.param\u003d\u003d\"p3\":\n\t\tself.getChild(\"root\").getChild(\"TextField\").props.text\u003d\"min.\"\n\t\n"
      },
      "persistent": true
    },
    "custom.unitSelected": {
      "binding": {
        "config": {
          "expression": "if({view.params.params.unitSelect},{view.params.params.dropOptions}[{/root/Dropdown.props.value}][\"label\"],{/root/TextField.props.text})"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "params.function": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.output": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.param": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.params": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.unit": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 404
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "60px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "\"Set \"+{view.params.param}+\":\""
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "textStyle": {
            "textAlign": "right"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "NumericEntryField"
        },
        "position": {
          "basis": "140px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "!{view.params.params.unitSelect}"
              },
              "type": "expr"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.output"
              },
              "type": "property"
            },
            "onChange": {
              "enabled": null,
              "script": "\t#sets the ramping time unit to coorespond to its integer value selected.\n\tif self.view.params.function\u003d\u003d2 and self.view.params.param\u003d\u003d\"p3\":\n\t\tif currentValue.value\u003d\u003d0:\n\t\t\tself.getSibling(\"TextField\").props.text\u003d\"/hr.\"\n\t\tif currentValue.value\u003d\u003d1:\n\t\t\tself.getSibling(\"TextField\").props.text\u003d\"/min.\"\n\t\tif currentValue.value\u003d\u003d2:\n\t\t\tself.getSibling(\"TextField\").props.text\u003d\"/sec.\""
            }
          }
        },
        "props": {
          "align": "left",
          "containerStyle": {
            "marginBottom": "2px",
            "marginTop": "2px"
          },
          "format": "0,0.###",
          "spinner": {
            "enabled": false
          },
          "style": {
            "marginLeft": "5px"
          }
        },
        "type": "ia.input.numeric-entry-field"
      },
      {
        "meta": {
          "name": "TextField"
        },
        "position": {
          "basis": "75px"
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "!{view.params.params.unitSelect}"
              },
              "type": "expr"
            }
          },
          "props.enabled": {
            "binding": {
              "config": {
                "expression": "if({view.params.function}\u003d3,false,if({view.params.function}\u003d4 \u0026\u0026 {view.params.param}\u003d\"p3\",false,true))"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.unit"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "placeholder": "unit",
          "style": {
            "marginBottom": "2px",
            "marginLeft": "2px",
            "marginTop": "2px"
          }
        },
        "type": "ia.input.text-field"
      },
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
          "basis": "150px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.params.unitSelect"
              },
              "type": "property"
            }
          },
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.params.dropOptions"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "../NumericEntryField.props.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "marginBottom": "2px",
            "marginLeft": "5px",
            "marginTop": "2px"
          }
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "borderStyle": "solid",
        "borderWidth": "1px",
        "margin": "2px"
      }
    },
    "type": "ia.container.flex"
  }
}