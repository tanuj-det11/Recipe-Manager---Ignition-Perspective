              "transforms": [
                {
                  "code": "\tif self.view.params.value.t1\u003d\u003d\"0\" or self.view.params.value.t1 is None:\n\t\treturn \"Tag Value: N/A\"\n\telif self.view.params.value.recipe_function!\u003d 7:\n\t\ttry:\t\t\n\t\t\treturn \"Tag Value: %s\" %round(system.tag.readBlocking([self.view.params.value.t1])[0].value,3)\n\t\texcept:\n\t\t\treturn \"Tag Value:\"\n\telse:\n\t\treturn \"Tag Value: %s\" %system.tag.readBlocking([self.view.params.value.t1])[0].value\n\t\t",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
